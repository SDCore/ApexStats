	"bugs": {
		"url": "https://github.com/StryderDev/Apex-Stats-Bot/issues"
	},
	"homepage": "https://github.com/StryderDev/Apex-Stats-Bot#readme",
	"dependencies": {
		"discord.js": "^14.6.0",
		"discord-api-types": "^0.37.18",
		"@discordjs/rest": "^1.3.0"
	},
	"devDependencies": {
		"rimraf": "^3.0.1"
	}
}
